@model List<Movie>;

@{
    var movies = Model;
}

<div class="card shadow">
    <div class="card-body">
        <ul class="list-group list-group-flush">
            @foreach (var movie in movies)
            {
                <li class="list-group-item">@movie.Title</li>
            }
        </ul>
    </div>
</div>